<script type="text/javascript">
autoCall();
function autoCall() {
    var url = "http://people.aero.und.edu/~spokharel/cgi-bin/513/1/ListGame.cgi";
    $.ajax({
           type: "POST",
           url: url,
           data: $("#developerSignup").serialize(), // serializes the form's elements.
           success:function(data)
           {    
		var arr = JSON.parse( data );		
           }
		
         });
};

</script>




# Compose a Java command.
  my @brands = ( 'MKF Studios', 'Wolf', 'Parabuster Inc.', 'Big Studios' );
  $cmd = "/usr/bin/java -Djava.security.egd=file:/dev/./urandom Check ";
  foreach my $brand (@brands) {
    if ( $query->param($brand) ) { $cmd .= "'" . $brand . "' "; }
  }

my @developers = $query->param( 'developers' );
  foreach my $developer (@developers) { $developerIds .= $developer . ","; }
$cmd   .= "'$developerIds' ";


















